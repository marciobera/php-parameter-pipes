{
  "name": "marciobera/php-parameter-pipes",
  "description": "Framework-agnostic PHP 8+ attribute-based parameter pipes (casters) with adapters for frameworks.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ParameterPipes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParameterPipes\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  }
}